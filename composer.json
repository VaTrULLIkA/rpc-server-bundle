{
  "name"       : "timiki/rpc-server-bundle",
  "type"       : "symfony-bundle",
  "description": "JSON-RPC server bundle for symfony",
  "keywords"   : [
    "rpc",
    "json-rpc",
    "bundle"
  ],
  "license"    : "MIT",
  "authors"    : [
    {
      "name" : "Netesin N",
      "email": "nikita@netesin.ru"
    }
  ],
  "require"    : {
    "php"                          : ">=5.5 || >=7.0",
    "doctrine/annotations"         : "1.*",
    "doctrine/cache"               : "1.*",
    "symfony/http-kernel"          : "~2.7 || ~3.0",
    "symfony/http-foundation"      : "~2.7 || ~3.0",
    "symfony/validator"            : "~2.7 || ~3.0",
    "symfony/dependency-injection" : "~2.7 || ~3.0",
    "symfony/event-dispatcher"     : "~2.7 || ~3.0",
    "symfony/serializer"           : "~2.7 || ~3.0",
    "symfony/stopwatch"            : "~2.7 || ~3.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "timiki/rpc-client"            : "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit"          : "5.3.*",
    "phpunit/php-code-coverage": "3.3.*"
  },
  "autoload"   : {
    "psr-4": {
      "Timiki\\Bundle\\RpcServerBundle\\": ""
    }
  }
}
