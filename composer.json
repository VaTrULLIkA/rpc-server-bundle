{
  "name"       : "timiki/rpc-server-bundle",
  "type"       : "symfony-bundle",
  "description": "JSON-RPC server bundle for symfony",
  "keywords"   : [
    "rpc",
    "json-rpc",
    "symfony-bundle"
  ],
  "license"    : "MIT",
  "authors"    : [
    {
      "name" : "Netesin N",
      "email": "nikita@netesin.ru"
    }
  ],
  "require"    : {
    "php"                          : "^7.1.3",
    "symfony/annotations-pack"     : "^1.0",
    "symfony/http-kernel"          : "^3.3",
    "symfony/http-foundation"      : "^3.3",
    "symfony/validator"            : "^3.3",
    "symfony/dependency-injection" : "^3.3",
    "symfony/event-dispatcher"     : "^3.3",
    "symfony/serializer"           : "^3.3",
    "symfony/stopwatch"            : "^3.3",
    "symfony/security-bundle"      : "^3.3",
    "sensio/framework-extra-bundle": "^3.0",
    "timiki/rpc-client"            : "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit"          : "^6.2",
    "phpunit/php-code-coverage": "^5.2"
  },
  "suggest"    : {
    "symfony/event-dispatcher": "",
    "symfony/filesystem"      : "",
    "symfony/process"         : "",
    "psr/log"                 : "For using the console logger"
  },
  "autoload"   : {
    "psr-4": {
      "Timiki\\Bundle\\RpcServerBundle\\": ""
    }
  },
  "config"     : {
    "bin-dir": "bin"
  }
}
